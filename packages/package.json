{
  "name": "overlay-kit",
  "version": "0.0.1",
  "description": "Next-generation tools for managing overlays",
  "keywords": [
    "overlay",
    "react"
  ],
  "homepage": "https://overlay-kit.slash.page",
  "bugs": "https://github.com/toss/overlay-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/toss/overlay-kit.git",
    "directory": "packages"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
